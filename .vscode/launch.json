    "version": "0.2.0",
    "configurations": [
        {
            "name": "UT CMD",
            "type": "debugpy",
            "request": "launch",
            "program": "cmd_run.py",
            "args": [
                "run",
                "usb"
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "reset_terminal",
            "cwd": "${workspaceFolder}/cmd/test/"
        },
        {
            "name": "CLI shell",
            "type": "debugpy",
            "request": "launch",
            "program": "cli_shell.py",
            "console": "integratedTerminal",
            "preLaunchTask": "",
            "postDebugTask": ""
        },
        {
            "name": "List",
            "type": "debugpy",
            "request": "launch",
            "program": "diag_cli.py",
            "console": "integratedTerminal",
            "args": [
                "list"
            ]
        },
        {
            "name": "run all",
            "type": "debugpy",
            "request": "launch",
            "program": "diag_cli.py",
            "console": "integratedTerminal",
            "args": [
                "run"
            ]
        }
    ]
}